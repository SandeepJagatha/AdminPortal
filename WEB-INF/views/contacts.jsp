

ul{
	list-style: none;padding-left: 0px;
}
li.alt {
    position: relative;
    padding: 4px 0 4px 0;
}
li.alt h4 {
        margin-bottom: 0px;
        -webkit-border-radius: 7px;*/
        -moz-border-radius: 7px;
        border-radius: 7px;
        background-color: #C7D0D5;
}
li.first h4 a{
    font-weight: bold;
    color: #000;
    display: block;
    text-decoration: none;
    font-size: 15px;
    line-height: 36px;
    text-indent: 10px;
    border: 1px solid #ccc;
    border-left: none;
    border-top: none;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    font-weight: normal;
    height: 1%;
    background-repeat: no-repeat;
}
div.actions {
    background: #F8F8F8;
    border: 1px solid #BBB;
    border-left: none;
    border-top: none;
    font-size: 100%;
    margin: 0 10px;
    padding: 0 13px;
    -webkit-border-bottom-left-radius: 15px;
    -webkit-border-bottom-right-radius: 15px;
    -moz-border-radius-bottomleft: 15px;
    -moz-border-radius-bottomright: 15px;
    border-bottom-left-radius: 15px;
    border-bottom-right-radius: 15px;
    display: none;
}
li.first .actions {
    display: block;
    zoom: 1;    
    -webkit-box-shadow: #A9C5DD 0px 1px 3px;
    -moz-box-shadow: #A9C5DD 0px 1px 3px;
    -o-box-shadow: #A9C5DD 0px 1px 3px;
    box-shadow: #A9C5DD 0px 1px 3px;
}
.actions a {
    display: inline-block;
    color: #444;
    line-height: 30px;
    font-size: 90%;
    text-decoration: none;
    margin: 0 0 0 -4px !important;
    padding: 0 15px 0 15px;
    font-weight: bold;
    background-repeat: no-repeat;
    font-size: 11px;
}
i{
	font-size: 14px !important;
}
.actions a:hover {
    background-color: #B9DCFF;
}
.dup, .del {
    float: right;
}
